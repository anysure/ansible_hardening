# {{ ansible_managed | comment }}
# Generated by Ansible role {{ ansible_role_name }}

log_file = /var/log/audit/audit.log
log_format = RAW
log_group = root
priority_boost = 4
flush = INCREMENTAL
freq = 20
num_logs = {{ hardening_auditd_num_logs }}
disp_qos = lossy
dispatcher = /sbin/audispd
name_format = NONE
##name = mydomain
max_log_file = {{ hardening_auditd_max_log_file }}
max_log_file_action = {{ hardening_auditd_max_log_file_action }}
space_left = {{ hardening_auditd_space_left }}
space_left_action = {{ hardening_auditd_space_left_action }}
action_mail_acct = {{ hardening_auditd_action_mail_acct }}
admin_space_left = 50
admin_space_left_action = {{ hardening_auditd_admin_space_left_action }}
disk_full_action = {{ hardening_auditd_disk_full_action }}
disk_error_action = {{ hardening_auditd_disk_error_action }}
##tcp_listen_port =
tcp_listen_queue = 5
tcp_max_per_addr = 1
##tcp_client_ports = 1024-65535
tcp_client_max_idle = 0
enable_krb5 = no
krb5_principal = auditd
##krb5_key_file = /etc/audit/audit.key